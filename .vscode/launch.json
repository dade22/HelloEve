{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Attach (Remote Debug)",
            "type": "python",
            "request": "attach",
            "localRoot": "${workspaceRoot}",
            "remoteRoot": "${workspaceRoot}",
            "port": 5000,
            "secret": "my_secret",
            "host": "localhost"
        },
        {
            "name": "Python",
            "type": "python",
            "request": "launch",
            "pythonPath": "/usr/bin/python3.5",
            "stopOnEntry": true,
            "program": "${file}",
            "debugOptions": [
                "WaitOnAbnormalExit",
                "WaitOnNormalExit",
                "RedirectOutput"
            ]
        }
    ],
    "compounds": []
}